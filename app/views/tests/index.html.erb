<%= link_to 'create Entry', new_test_path %></br></br>


<div>
  <table>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Created At</th>
    </tr>
    <%unless @tests.blank? %>
        <% @tests.each do |test| %>
            <tr>
              <td><%= test.id %></td>
              <td><%= test.name %></td>
              <td style="padding-left: 30px"><%= test.created_at %></td>
            </tr>
        <% end %>
    <% end %>
  </table>
</div>